{
  "name": "@ckapp/math",
  "version": "0.2.0-next.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./geometry/Circle": {
      "types": "./dist/lib/geometry/Circle.d.ts",
      "import": "./dist/lib/geometry/Circle.js",
      "default": "./dist/lib/geometry/Circle.js"
    },
    "./geometry/Cone": {
      "types": "./dist/lib/geometry/Cone.d.ts",
      "import": "./dist/lib/geometry/Cone.js",
      "default": "./dist/lib/geometry/Cone.js"
    },
    "./geometry/Cylinder": {
      "types": "./dist/lib/geometry/Cylinder.d.ts",
      "import": "./dist/lib/geometry/Cylinder.js",
      "default": "./dist/lib/geometry/Cylinder.js"
    },
    "./geometry/Sphere": {
      "types": "./dist/lib/geometry/Sphere.d.ts",
      "import": "./dist/lib/geometry/Sphere.js",
      "default": "./dist/lib/geometry/Sphere.js"
    },
    "./geometry/euclidean/Plane": {
      "types": "./dist/lib/geometry/euclidean/Plane.d.ts",
      "import": "./dist/lib/geometry/euclidean/Plane.js",
      "default": "./dist/lib/geometry/euclidean/Plane.js"
    },
    "./geometry/Trigonometry": {
      "types": "./dist/lib/geometry/Trigonometry.d.ts",
      "import": "./dist/lib/geometry/Trigonometry.js",
      "default": "./dist/lib/geometry/Trigonometry.js"
    },
    "./numbers/Quaternion": {
      "types": "./dist/lib/numbers/Quaternion.d.ts",
      "import": "./dist/lib/numbers/Quaternion.js",
      "default": "./dist/lib/numbers/Quaternion.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "peerDependencies": {
    "effect": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckapps/ts-libs",
    "directory": "packages/math"
  }
}
